{
  "name": "icon-packs",
  "version": "0.8.0",
  "description": "Icons for react",
  "main": "index.js",
  "author": "Barry de Kleijn <kleijn.barry@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clear": "yarn ts scripts/clear.ts",
    "prebuild": "yarn clear && yarn ts src/buildScripts/index.ts",
    "build:es6": "tsc",
    "build:cjs": "tsc -m commonjs --outDir cjs",
    "build": "yarn build:es6 && yarn build:cjs",
    "postbuild": "yarn ts scripts/postBuild.ts",
    "prepublish": "yarn test:ci && yarn build",
    "postpublish": "yarn clear",
    "test": "jest --watch",
    "test:ci": "jest",
    "ts": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Barry127/icons-pack.git"
  },
  "bugs": {
    "url": "https://github.com/Barry127/icons-pack/issues"
  },
  "devDependencies": {
    "@ant-design/icons-svg": "^4.0.0-rc.0",
    "@blueprintjs/icons": "^3.13.0",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@primer/octicons": "^9.3.1",
    "@types/feather-icons": "^4.7.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "@types/rimraf": "^2.0.2",
    "@types/xmldom": "^0.1.29",
    "bootstrap-icons": "^1.0.0-alpha2",
    "camelcase": "^5.3.1",
    "chalk": "^2.4.2",
    "devicons": "^1.8.0",
    "feather-icons": "^4.25.0",
    "ionicons": "^4.6.3",
    "jest": "^24.9.0",
    "line-awesome": "^1.3.0",
    "material-design-icons": "^3.0.1",
    "remixicon": "^2.2.0",
    "rimraf": "^3.0.0",
    "simple-icons": "^2.3.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "typicons.font": "^2.0.9",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "@types/react": "^16.9.9"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "modulePaths": [
      "src"
    ]
  }
}
