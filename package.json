{
  "name": "icon-packs",
  "version": "0.0.2",
  "description": "Icons for react",
  "main": "index.js",
  "author": "Barry de Kleijn <kleijn.barry@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clear": "rimraf index.* types.* blueprint devicons feather octicons remix typicons",
    "prebuild": "npm run clear",
    "build": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node src/buildScripts/index.ts && tsc",
    "postbuild": "rimraf src/blueprint src/devicons src/feather src/octicons src/remix src/typicons",
    "prepublish": "npm run test:ci & npm run build",
    "postpublish": "npm run clear",
    "test": "jest --watch",
    "test:ci": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Barry127/icons-pack.git"
  },
  "bugs": {
    "url": "https://github.com/Barry127/icons-pack/issues"
  },
  "devDependencies": {
    "@blueprintjs/icons": "^3.11.0",
    "@primer/octicons": "^9.1.1",
    "@types/feather-icons": "^4.7.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/xmldom": "^0.1.29",
    "camelcase": "^5.3.1",
    "chalk": "^2.4.2",
    "devicons": "^1.8.0",
    "feather-icons": "^4.24.1",
    "jest": "^24.9.0",
    "remixicon": "^2.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "typicons.font": "^2.0.9",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "@types/react": "^16.9.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "modulePaths": [
      "src"
    ]
  }
}
